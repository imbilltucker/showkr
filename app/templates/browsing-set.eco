<div class="span6">
  <h3><a href="#<%- @model.id %>"><%- @model.title() %></a></h3>
  <small><%- @model.description() %></small>
  <dl>
    <dt>Created at</dt>
    <dd><%- @model.date() %></dd>
    <dt>In total</dt>
    <dd><%- @model.photos() %> photos</dd>
  </dl>
</div>
<div class="span6">
  <a href="#<%- @model.id %>">
    <img src="<%- @photo.small() %>">
  </a>
</div>
