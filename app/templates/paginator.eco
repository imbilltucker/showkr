<% if @length > @pageBy: %>
<ul>
  <li class="prev <% if not @hasPrev: %>disabled<% end %>"><a href="#">&larr;</a></li>
  <li class="disabled">
    <a href="#"><%- @start + 1 %>  - <%- @end %> of <%- @length %></a>
  </li>
  <li class="next <% if not @hasNext: %>disabled<% end %>"><a href="#">&rarr;</a></li>
</ul>
<% end %>
