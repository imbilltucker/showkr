<% if @length > @pageBy: %>
<li class="previous">
  <a href="#" class="<% if not @hasPrev: %>disabled<% end %>">&larr;</a>
</li>
<li>
  <a href="#" class="disabled noborder"><%- @start + 1 %>  - <%- @end %> of <%- @length %></a>
</li>
<li class="next">
  <a href="#" class="<% if not @hasNext: %>disabled<% end %>">&rarr;</a>
</li>
<% end %>
